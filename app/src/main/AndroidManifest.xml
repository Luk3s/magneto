<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unipi.luk3s.magnetotest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IdentificationExampleActivity" />
        <activity android:name=".PositionExampleActivity" />
        <activity android:name=".VirtualTapExampleActivity" />
        <activity android:name=".RadialPositionExampleActivity" />
        <activity android:name=".LinearMotionExampleActivity" />
        <activity android:name=".RapidChangesExampleActivity" />
        <activity android:name=".DisplaySensorDataActivity" />
        <activity android:name=".CollectSensorDataActivity" />
        <activity android:name=".RadialCubeActivity"></activity>
    </application>

</manifest>
